{
  "name": "client",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:staging": "ng build --configuration staging --output-path dist/staging",
    "postbuild:staging": "cp src/www/index.html dist/staging/index.html && cp src/www/index.html dist/staging/404.html",
    "build:production": "ng build --configuration production --output-path dist/production",
    "postbuild:production": "cp src/www/index.html dist/production/index.html && cp src/www/index.html dist/production/404.html",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint --fix",
    "lint:ci": "ng lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "bundle-visualizer": "ng build --named-chunks && cd ./dist/client/browser && npx vite-bundle-visualizer && cd -"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.8",
    "@angular/cdk": "^17.0.4",
    "@angular/common": "^17.0.8",
    "@angular/compiler": "^17.0.8",
    "@angular/core": "^17.0.8",
    "@angular/forms": "^17.0.8",
    "@angular/material": "^17.0.4",
    "@angular/platform-browser": "^17.0.8",
    "@angular/platform-browser-dynamic": "^17.0.8",
    "@angular/router": "^17.0.8",
    "@fontsource/nunito": "^5.0.16",
    "canvas-confetti": "^1.9.2",
    "firebase": "^10.7.1",
    "material-symbols": "^0.14.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-builders/jest": "^17.0.0",
    "@angular-devkit/build-angular": "^17.0.8",
    "@angular-eslint/builder": "17.1.1",
    "@angular-eslint/eslint-plugin": "17.1.1",
    "@angular-eslint/eslint-plugin-template": "17.1.1",
    "@angular-eslint/schematics": "17.1.1",
    "@angular-eslint/template-parser": "17.1.1",
    "@angular/cli": "^17.0.8",
    "@angular/compiler-cli": "^17.0.8",
    "@angular/localize": "^17.0.8",
    "@types/canvas-confetti": "^1.6.4",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "6.17.0",
    "@typescript-eslint/parser": "6.17.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "jest": "^29.7.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.10",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.2.2"
  }
}
